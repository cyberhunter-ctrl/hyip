{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "bacon/bacon-qr-code": "^2.0",
        "charlesassets/laravel-perfectmoney": "^1.0",
        "coderflex/laravel-ticket": "^1.0",
        "coingate/coingate-php": "^4.1",
        "flutterwavedev/flutterwave-v3": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "joedixon/laravel-translation": "^2.1",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "mckenziearts/laravel-notify": "^2.3",
        "mews/purifier": "^3.3",
        "mollie/laravel-mollie": "^2.0",
        "pragmarx/google2fa-laravel": "^2.0",
        "pragmarx/google2fa-qrcode": "^3.0",
        "rickselby/laravel-gate-cache": "^3.5",
        "shakurov/coinbase": "^0.7.7",
        "spatie/laravel-permission": "^5.5",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^9.8",
        "unicodeveloper/laravel-paystack": "1.0.*",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "classmap": [
            "modules/payment"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
